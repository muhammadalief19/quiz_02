<!DOCTYPE html>
<html lang="zxx">
  <head>
    <title><%= title %></title>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="author" content="" />
    <meta name="MobileOptimized" content="320" />
    <!--Start Style -->
    <link rel="stylesheet" type="text/css" href="/assets/css/fonts.css" />
    <link
      rel="stylesheet"
      type="text/css"
      href="/assets/css/bootstrap.min.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/assets/css/font-awesome.min.css"
    />
    <link rel="stylesheet" type="text/css" href="/assets/css/icofont.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css" />
    <link rel="stylesheet" id="theme-change" type="text/css" href="#" />
    <!-- Favicon Link -->
    <link
      rel="shortcut icon"
      type="image/png"
      href="/assets/images/favicon.png"
    />
  </head>

  <body>
    <div class="loader">
      <div class="spinner">
        <img src="/assets/images/loader.gif" alt="" />
      </div>
    </div>
    <!-- Main Body -->
    <div class="page-wrapper">
      <!-- Header Start -->
      <%- include("../partials/_header") %>
      <!-- Sidebar Start -->
      <%- include("../partials/_sidebar") %>
      <!-- Sidebar End -->

      <!-- Container Start -->
      <div class="page-wrapper">
        <div class="main-content">
          <!-- Page Title Start -->
          <div class="row">
            <div class="colxl-12 col-lg-12 col-md-12 col-sm-12 col-12">
              <div class="page-title-wrapper">
                <div class="page-title-box">
                  <h4 class="page-title bold"><%= title %></h4>
                </div>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="card">
              <div class="card-header">
                <h4>Create Pemilik</h4>
              </div>
              <div class="card-body">
                <form method="POST" action="/kapal/store">
                  <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="form-group">
                      <label class="col-form-label">Nama Kapal</label>
                      <input
                        class="form-control"
                        type="text"
                        placeholder="example"
                        name="nama_kapal"
                        autocomplete="off"
                      />
                    </div>
                    <div class="form-group s-opt">
                      <label for="id_alat" class="col-form-label"
                        >Alat Tangkap</label
                      >
                      <select
                        class="select2 form-control select-opt"
                        id="id_alat"
                        name="id_alat"
                      >
                        <% for(var i = 0; i < alat.length; i++){ %>

                        <option value="<%= alat[i].id_alat %>">
                          <%= alat[i].nama_alat %>
                        </option>
                        <% } %>
                      </select>
                      <span class="sel_arrow mr-2">
                        <i class="fa fa-angle-down"></i>
                      </span>
                    </div>
                    <div class="form-group s-opt">
                      <label for="id_pemilik" class="col-form-label"
                        >Pemilik</label
                      >
                      <select
                        class="select2 form-control select-opt"
                        id="id_pemilik"
                        name="id_pemilik"
                      >
                        <% for(var i = 0; i < pemilik.length; i++){ %>

                        <option value="<%= pemilik[i].id_pemilik %>">
                          <%= pemilik[i].nama_pemilik %>
                        </option>
                        <% } %>
                      </select>
                      <span class="sel_arrow mr-2">
                        <i class="fa fa-angle-down"></i>
                      </span>
                    </div>
                    <div class="form-group s-opt">
                      <label for="id_dpi" class="col-form-label">DPI</label>
                      <select
                        class="select2 form-control select-opt"
                        id="id_dpi"
                        name="id_dpi"
                      >
                        <% for(var i = 0; i < dpi.length; i++){ %>

                        <option value="<%= dpi[i].id_dpi %>">
                          <%= dpi[i].nama_dpi %>
                        </option>
                        <% } %>
                      </select>
                      <span class="sel_arrow mr-2">
                        <i class="fa fa-angle-down"></i>
                      </span>
                    </div>
                    <div class="form-group mb-0">
                      <button class="btn btn-success" type="submit">
                        create
                      </button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>

          <div class="ad-footer-btm">
            <p>Copyright 2024 Â© JI BAKRIE.</p>
          </div>
        </div>
      </div>
    </div>

    <!-- Script Start -->
    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/popper.min.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/swiper.min.js"></script>
    <script src="/assets/js/apexchart/apexcharts.min.js"></script>
    <script src="/assets/js/apexchart/control-chart-apexcharts.js"></script>
    <!-- Page Specific -->
    <script src="/assets/js/nice-select.min.js"></script>
    <!-- Custom Script -->
    <script src="/assets/js/custom.js"></script>
  </body>

